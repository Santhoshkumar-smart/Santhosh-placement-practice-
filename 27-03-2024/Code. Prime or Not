#include <stdio.h>
int main() {
   int n, i, s=0;
   scanf("%d",&n);
   for(i=2; i<= n/2; ++i) {
      if(n%i == 0) {
         s=1;
         break;
      }
   }
   if (n==1) {
      printf("not prime and composite");
   }
   else {
       if (s== 0)
       {
         printf("prime number");
       }
      else
      {
         printf(" not prime number");
      }
   }
}
