#include <stdio.h>
int main()
{
    int n,sum=0,as;
    scanf("%d",&n);
    while(n>0)
    {
        as=n%10;
        if(as%2==1)
        {
            sum=sum+as;
        }
        n=n/10;
    }
    if(sum%2==0)
    {
        printf("Lucky number");
    }
    else
    {
        printf("Not lucky number");
    }
    return 0;
}
