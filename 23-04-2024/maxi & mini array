arr = [3, 7, 1, 5, 9, 2, 8, 4, 6]

max_element = arr[0]
min_element = arr[0]

for num in arr:
    if num > max_element:
        max_element = num  # Update max_element if a larger element is found
    elif num < min_element:
        min_element = num  # Update min_element if a smaller element is found

print("Array:", arr)
print("Maximum element:", max_element)
print("Minimum element:", min_element)
